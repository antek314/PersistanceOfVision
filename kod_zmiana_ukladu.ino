void WyswietlBitmape(int j, int i, uint32_t c)
{
  if((bitmapa_przeksztalcona[j][i+9]==1) && (j!=107))
  {
    strip.setPixelColor(i, c);
  }
  else
  {
    strip.setPixelColor(i, 0);
  }
}

void PrzeksztalcBitmape()
{
  for(int a = 0; a<49; a++)
  {
    for(int b = 0; b < 49; b++)
    {
      if(bitmapa[a][b]==1)
      {
        int x = a-24;
        int y = b-24;
          float phi;
          if((x>0) && (y>=0))
          {
            phi = atan((float)y/x);
          }
          if((x>0) && (y<0))
          {
            phi = atan((float)y/x) + 6.28;
          }
          if(x<0)
          {
            phi = atan((float)y/x) + 3.14;
          }
          if((x==0) && (y>0))
          {
            phi = 3.14/2;
          }
          if((x==0) && (y<0))
          {
            phi = 3.14 * 3/2;
          }
          int promien = (int)round(sqrt((x * x) + (y * y)));
          int wyn = (int)round(phi / 0.05815);
          bitmapa_przeksztalcona[wyn][promien] = 1;
      }
    }
  }
}