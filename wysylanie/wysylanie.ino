// Twoja tablica
//uint8_t data[108][34][3];  // MUSI być uint8_t (0–255)

uint8_t bitmapa_przeksztalcona[108][34][3] = {0};


const int TOTAL_BYTES = sizeof(bitmapa_przeksztalcona);   // 11016 bajtów
const int PACKET_SIZE = 512;

IPAddress serverIP(192,168,4,1);

void setup() {
  Serial.begin(115200);
  delay(1000);

  // przykładowe dane
  // for (int i=0; i<108; i++)
  //   for (int j=0; j<34; j++)
  //     for (int k=0; k<3; k++)
  //       data[i][j][k] = (i+j+k) % 256;

  PrzeksztalcBitmape();

  Serial.println("Łączenie z ESP-C3...");
  WiFi.begin(ssid, password);

  while (WiFi.status() != WL_CONNECTED) {
    delay(200);
    Serial.print(".");
  }

  Serial.println("\nPołączono!");
  Serial.print("IP ESP8266: ");
  Serial.println(WiFi.localIP());

  Udp.begin(4211);  // port nadawczy (może być dowolny)

  delay(1000);
}

void loop() {
  if (digitalRead(D1) == HIGH) {
    wyslij();
  }
  delay(100);
}

void wyslij(){
  Serial.println("=== Wysyłam tablicę... ===");

  int bytesSent = 0;

  while (bytesSent < TOTAL_BYTES) {
    int chunkSize = PACKET_SIZE;

    // Jeżeli ostatnia paczka jest mniejsza
    if (bytesSent + chunkSize > TOTAL_BYTES) {
      chunkSize = TOTAL_BYTES - bytesSent;
    }

    Udp.beginPacket(serverIP, SERVER_PORT);
    Udp.write(&bitmapa_przeksztalcona[0][0][0] + bytesSent, chunkSize);
    Udp.endPacket();

    Serial.print("Wysłano paczkę: ");
    Serial.print(chunkSize);
    Serial.print(" bajtów (łącznie: ");
    Serial.print(bytesSent + chunkSize);
    Serial.println(")");

    bytesSent += chunkSize;
    delay(10);   // lekka przerwa dla stabilności
  }

  Serial.println("Oczekiwanie na DONE...");

  unsigned long start = millis();
  while (millis() - start < 2000) {
    int packetSize = Udp.parsePacket();
    if (packetSize) {
      char reply[20];
      int len = Udp.read(reply, sizeof(reply));
      reply[len] = 0;
      Serial.print("Odpowiedź od ESP-C3: ");
      Serial.println(reply);
      break;
    }
  }

  Serial.println("Koniec transmisji.\n");
  delay(2000);
}

void PrzeksztalcBitmape()
{
  for(int a = 0; a<49; a++)
  {
    for(int b = 0; b < 49; b++)
    {
      if((bitmapa[a][b][0]>0) || (bitmapa[a][b][1]>0) || (bitmapa[a][b][2]>0))
      {
        int x = a-24;
        int y = b-24;
          double phi;
          if((x>0) && (y>=0))
          {
            phi = atan((double)y/x);
          }
          if((x>0) && (y<0))
          {
            phi = atan((double)y/x) + 6.2831853;
          }
          if(x<0)
          {
            phi = atan((double)y/x) + 3.14159265;
          }
          if((x==0) && (y>0))
          {
            phi = 3.14159265/2;
          }
          if((x==0) && (y<0))
          {
            phi = 3.14159265 * 3/2;
          }
          int promien = (int)round(sqrt((x * x) + (y * y)));
          int wyn = (int)round(phi / 0.0581481481); // 2*pi/108
          bitmapa_przeksztalcona[wyn][promien][0] = bitmapa[a][b][0];
          bitmapa_przeksztalcona[wyn][promien][1] =  bitmapa[a][b][1];
          bitmapa_przeksztalcona[wyn][promien][2] =  bitmapa[a][b][2];
      }
    }
  }
}
void Filtr(){
  for(int a = 0; a<108; a++)
  {
    for(int b = 0; b < 34; b++)
    {
      for(int c = 0; c<3; c++)
      {
        if(a > 0 && a < 107)
        {
          if((bitmapa_przeksztalcona[a-1][b][c] > 0) && (bitmapa_przeksztalcona[a-1][b][c] == bitmapa_przeksztalcona[a+1][b][c]))
          {
            bitmapa_przeksztalcona[a][b][c] = bitmapa_przeksztalcona[a-1][b][c];
          }
        }

      }
    }
  }
}
